{
    "data": {
        "type": "graph",
        "bs": 32
    },
    "model": {
        "type": "graph_baseline", 
        "name": "graph_baseline",
        "hidden_dim": 64
    },
    "train": {
        "epochs": 50,
        "lr": 0.001,
        "weight_decay": 0.0001,
        "betas": [0.9, 0.999],
        "step_size": 20,
        "gamma": 0.1,
        "save": false
    },
    "eval": {
        "data": "test",
        "plot_history": true,
        "evaluate": true,
        "static_predict": true,
        "image_path": "tests/A.jpg",
        "realtime_predict": true,
        "confusion_matrix": true,
        "save": false
    }
}